import cv2

def apply_canny_outline(image_path, output_path, threshold1=100, threshold2=200):
    image = cv2.imread(image_path, cv2.IMREAD_GRAYSCALE)
    edges = cv2.Canny(image, threshold1, threshold2)
    cv2.imwrite(output_path, edges)

if __name__ == "__main__":
    input_image_path = "input_image.jpg"
    output_image_path = "outline_image.jpg"
    apply_canny_outline(input_image_path, output_image_path, threshold1=100, threshold2=200)
